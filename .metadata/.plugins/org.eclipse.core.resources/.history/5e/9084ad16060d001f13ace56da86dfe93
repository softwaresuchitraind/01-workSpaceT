package com.sukla.threads;

import java.util.function.Function;
import java.util.stream.Collectors;

public class M2
{
	public static void main(String[] args)
	{
		String s1= "hello";
		

	s1.chars().map(s->(char)s).collect(Function.identity(),Collectors.counting()).forEach(System.out::println);
	}
}
